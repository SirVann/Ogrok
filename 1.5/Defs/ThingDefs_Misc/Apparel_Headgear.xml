<?xml version="1.0" encoding="utf-8" ?>
<Defs>

 <ThingDef ParentName="HatMakeableBase">
    <defName>DA_GreatJaw</defName>
    <label>great jaw</label>
    <description>A large collar fashioned to ressemble a massive jaw. A display of strength and brutality.</description>
    <graphicData>
      <texPath>Things/Pawn/Humanlike/Headgear/GreatJaw/GreatJaw</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
	
	<techLevel>Medieval</techLevel>
	
    <costStuffCount>25</costStuffCount>
    <stuffCategories>
      <li>Metallic</li>
    </stuffCategories>
	
    <statBases>
      <MaxHitPoints>120</MaxHitPoints>
      <WorkToMake>1800</WorkToMake>
      <Mass>0.5</Mass>
      <StuffEffectMultiplierArmor>0.65</StuffEffectMultiplierArmor>
      <StuffEffectMultiplierInsulation_Cold>0.00</StuffEffectMultiplierInsulation_Cold>
      <StuffEffectMultiplierInsulation_Heat>0.00</StuffEffectMultiplierInsulation_Heat>
      <EquipDelay>1.5</EquipDelay>
    </statBases>
	
    <equippedStatOffsets>
			<PainShockThreshold>0.10</PainShockThreshold>
			<SocialImpact>0.10</SocialImpact>
			<EatingSpeed>0.10</EatingSpeed>
    </equippedStatOffsets>
	
    <recipeMaker>
      <researchPrerequisite>Smithing</researchPrerequisite>
      <recipeUsers>
        <li>ElectricSmithy</li>
        <li>FueledSmithy</li>
      </recipeUsers>
      <displayPriority>200</displayPriority>
    </recipeMaker>
	
    <apparel>
      <countsAsClothingForNudity>false</countsAsClothingForNudity>
      <bodyPartGroups>
        <li>Neck</li>
      </bodyPartGroups>
      <wornGraphicPaths>
        <li>Things/Pawn/Humanlike/Headgear/GreatJaw/GreatJawA</li>
        <li>Things/Pawn/Humanlike/Headgear/GreatJaw/GreatJawB</li>
        <li>Things/Pawn/Humanlike/Headgear/GreatJaw/GreatJawC</li>
      </wornGraphicPaths>
      <layers>
        <li>Overhead</li>
      </layers>
      <renderSkipFlags>
        <li>Beard</li>
      </renderSkipFlags>
      <canBeGeneratedToSatisfyWarmth>false</canBeGeneratedToSatisfyWarmth>
      <canBeGeneratedToSatisfyToxicEnvironmentResistance>false</canBeGeneratedToSatisfyToxicEnvironmentResistance>
	 
		
      <drawData>
        <defaultData>
          <layer>71</layer>
        </defaultData>
      </drawData>
    </apparel>
  </ThingDef>
  
